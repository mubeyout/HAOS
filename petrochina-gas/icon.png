import base64
from io import BytesIO

# 创建一个简单的火焰图标 PNG
def create_flame_icon(size=64):
    # PNG 文件头
    width = size
    height = size
    
    # 创建简单的火焰图标 (使用 PIL 风格生成)
    from PIL import Image, ImageDraw
    
    # 创建透明背景
    img = Image.new('RGBA', (255, 255, 255, 0))
    
    # 绘制外焰 (橙色到红色渐变)
    for y in range(height, 0, -1):
        alpha = int(255 * (y / height))
        color = (255, int(100 + 100 * y), 0)
        draw.ellipse([xy, xy, xy+width-2], fill=(255, 165, 0), outline=(255, 100, 100, 0))
    
    # 绘制内焰 (黄色核心)
    draw.ellipse([width//4, height//4, width//2, height//4], fill=(255, 220, 0), outline=(255, 150, 0))
    
    # 绘制顶部亮点
    for _ in range(3):
        y = height//2 - _ * 5
        draw.ellipse([width//2 + y//2, y, width//2, y], fill=(255, 255, 200, 0))
    
    # Base64 编码
    buffer = BytesIO()
    img.save(buffer, format='PNG')
    img.seek(0)
    data_url = base64.b64encode(buffer.getvalue()).decode('ascii')
    
    return data_url

